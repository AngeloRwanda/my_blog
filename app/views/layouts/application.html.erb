<!DOCTYPE html>
<html>
<head>
	<title>MyBlog</title>
	<%= csrf_meta_tags %>
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>

	<% if current_user -%>

	<%= link_to "Log Out",destroy_user_session_path, method: :delete, class: "is-danger" %>
	<% end -%>

	<%= yield %>
</body>
</html>
